{
  "name": "@onboarding/logger",
  "version": "0.0.0",
  "description": "logger package for onboarding turborepo",
  "author": "hirzinurfakhrian@gmail.com",
  "type": "module",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.5.3",
  "main": "dist/index.cjs",
